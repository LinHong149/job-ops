# Copy this file to .env and fill in your actual values

# Notion API
NOTION_TOKEN=your_notion_integration_token_here
NOTION_DB_ID=your_notion_database_id_here

# Discord Webhook
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here

# Gmail API (from Google Cloud Console)
GMAIL_CLIENT_ID=your_gmail_client_id_here
GMAIL_CLIENT_SECRET=your_gmail_client_secret_here
GMAIL_REFRESH_TOKEN=your_gmail_refresh_token_here

# Optional: Gmail Labels (defaults provided)
GMAIL_LABEL_OA=OA
GMAIL_LABEL_INTERVIEW=Interview
GMAIL_LABEL_REJECTED=Rejected

# Optional: Cron schedules (defaults provided)
CRON_GMAIL=*/5 * * * *
CRON_WEEKLY_ANALYTICS=0 18 * * SUN

# MCP WebSocket port
MCP_WS_PORT=8719
